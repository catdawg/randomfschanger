{
  "name": "randomfschanger",
  "version": "0.0.2",
  "description": "Change a folder randomly by adding/removing/editing files and directories over a specified period of time. Used for stress testing filesystem watching utilities.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --forceExit --coverage --verbose -i",
    "codacy": "jest --forceExit --coverage --verbose && cat ./coverage/lcov.info | codacy-coverage",
    "build-ts": "tsc"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/catdawg/randomfschanger.git"
  },
  "keywords": [
    "fs",
    "testing",
    "test",
    "random"
  ],
  "author": "Rui Campos <ruir2c@gmail.com> (http://github.com/catdawg)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/catdawg/randomfschanger/issues"
  },
  "homepage": "https://github.com/catdawg/randomfschanger#readme",
  "engines": {
    "node": ">=7.9.0"
  },
  "dependencies": {
    "verror": "^1.10.0",
    "winston": "^2.4.0",
    "fs-extra": "^4.0.3",
    "semaphore-async-await": "^1.5.1",
    "chance": "^1.0.16"
  },
  "devDependencies": {
    "@types/chance": "1.0.0",
    "@types/chai": "^4.1.2",
    "@types/chokidar": "^1.7.5",
    "@types/fs-extra": "^5.0.1",
    "@types/jest": "^22.2.0",
    "@types/tmp": "^0.0.33",
    "@types/verror": "^1.10.3",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.3",
    "jest": "^22.4.2",
    "sinon": "^4.1.2",
    "source-map-support": "^0.5.3",
    "tmp": "^0.0.33",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
